r0 = creat(&(0x7f0000000040)='./file0\x00', 0x0)
r1 = open(&(0x7f0000000380)='./file0\x00', 0x0, 0x0)
r2 = eventfd2(0x0, 0x0)
r3 = eventfd2(0x0, 0x0)
ftruncate(r0, 0x7fffffe)
write$binfmt_script(r0, &(0x7f0000000000)=ANY=[@ANYRESDEC, @ANYRES32=r3, @ANYRES64], 0x22)
sendfile(r2, r1, 0x0, 0x200000000026)
