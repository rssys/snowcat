r0 = openat$uhid(0xffffff9c, &(0x7f0000000000)='/dev/uhid\x00', 0x0, 0x0)
r1 = openat$uhid(0xffffff9c, &(0x7f0000000000)='/dev/uhid\x00', 0x2, 0x0)
write$UHID_CREATE2(r1, &(0x7f0000000140)={0xb, {'syz0\x00', 'syz0\x00', 'syz1\x00', 0x1, 0x0, 0x0, 0x0, 0x0, 0x0, "ec"}}, 0x119)
write$UHID_DESTROY(r1, &(0x7f0000000040), 0x4)
write$UHID_CREATE2(r0, 0x0, 0x119)
